<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

    <name>GabineteNozinho</name>

	<application>
		<resource-bundle>
			<base-name>br.com.nozinho.global</base-name>
			<var>global</var>
		</resource-bundle>
		
		<resource-bundle>
			<base-name>br.com.nozinho.messages</base-name>
			<var>messages</var>
		</resource-bundle>
		
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config> 
			
				
	</application>

<!-- 	<lifecycle> -->
<!-- 		<phase-listener>br.com.nozinho.web.listener.Listener</phase-listener> -->
<!-- 	</lifecycle> -->
	
</faces-config>
